{
  "name": "urban-twin",
  "version": "1.0.0",
  "description": "Urban twin web component made with lit",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "start": "parcel index.html"
  },
  "keywords": [
    "cesium",
    "lit",
    "parcel"
  ],
  "author": "Emanuele Mastaglia",
  "license": "ISC",
  "type": "module",
  "module": "dist/cesium-viewer.js",
  "source": "src/cesium-viewer.js",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@parcel/transformer-inline-string": "^2.8.3",
    "del-cli": "^5.0.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-custom-elements": "^0.0.7",
    "make-dir-cli": "^3.0.0",
    "parcel": "latest",
    "parcel-reporter-static-files-copy": "^1.5.0",
    "prettier": "^2.8.6"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "boxicons": "^2.1.4",
    "cesium": "^1.104.0",
    "lit": "^2.7.0"
  },
  "browser": {
    "assert": false,
    "buffer": false,
    "crypto": false,
    "fs": false,
    "http": false,
    "https": false,
    "path": false,
    "process": false,
    "querystring": false,
    "stream": false,
    "url": false,
    "util": false,
    "zlib": false
  },
  "staticFiles": [
    {
      "staticOutPath": "static/Data",
      "staticPath": "src/assets/data"
    },
    {
      "staticOutPath": "static/img",
      "staticPath": "src/assets/img"
    },
    {
      "staticOutPath": "static/icons",
      "staticPath": "src/assets/icons"
    },
    {
      "staticOutPath": "static/Assets",
      "staticPath": "node_modules/cesium/Build/Cesium/Assets"
    },
    {
      "staticOutPath": "static/ThirdParty",
      "staticPath": "node_modules/cesium/Build/Cesium/ThirdParty"
    },
    {
      "staticOutPath": "static/Widgets",
      "staticPath": "node_modules/cesium/Build/Cesium/Widgets"
    },
    {
      "staticOutPath": "static/Workers",
      "staticPath": "node_modules/cesium/Build/Cesium/Workers"
    }
  ]
}
